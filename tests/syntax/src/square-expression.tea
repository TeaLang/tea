
items = ['a', 'b']

// push, append at the end
items[] = 'c'

var_dump(items)

// pop, take at the end
last_element = items[]

var_dump(last_element)

// unshift, insert at the begin
[]items = 'd'

var_dump(items)

// shift, take at the begin
first_element = []items

var_dump(first_element, items)
