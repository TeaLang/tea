
#main

str = '\'abc123\'';
match_count = preg_match(/^\\'[a-z0-9]+\'$/i, str)
echo match_count  // 0 or 1

regex = /[\s\|,]/
result0 = regex.capture_one('ab cd|e,f') // result: [' ']
var_dump(result0)

result1 = regex.capture_all('ab cd|e,f') // result: [' ', '|', ',']
var_dump(result1)

// use the regex to separate a text
result2 = regex.separate('ab cd|e,f') // result: ['ab', 'cd', 'e', 'f']
var_dump(result2)

// test a string
is_matched = /^a-z$/iu.test('abcABC')
var_dump(is_matched)

